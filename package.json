{
  "private": true,
  "packageManager": "pnpm@8.7.6",
  "scripts": {
    "cz": "czg",
    "lint": "eslint . --ext .vue,.js,.ts,.tsx,.md,.json --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "pnpm run -r --parallel stub",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@tadmin/assets": "workspace:*",
    "@tadmin/store": "workspace:*",
    "@tadmin/vite-config": "workspace:*",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "cz-git": "^1.7.1",
    "czg": "^1.7.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsonc": "^2.9.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "pinia": "^2.1.6",
    "prettier": "^3.0.3",
    "tdesign-vue-next": "^1.5.7",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vue": "^3.3.4"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,tsx,json,md}": "eslint --fix"
  }
}
